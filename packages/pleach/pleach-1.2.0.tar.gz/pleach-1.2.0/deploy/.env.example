DOMAIN=localhost
STACK_NAME=pleach-stack

TRAEFIK_PUBLIC_NETWORK=traefik-public
TRAEFIK_TAG=pleach-traefik
TRAEFIK_PUBLIC_TAG=traefik-public


# Pleach backend configuration
PLEACH_LOG_LEVEL=debug
PLEACH_SUPERUSER=superuser
PLEACH_SUPERUSER_PASSWORD=superuser
PLEACH_NEW_USER_IS_ACTIVE=False


# Pleach frontend configuration
BACKEND_URL=http://backend:7860

# RabbitMQ configuration
RABBITMQ_DEFAULT_USER=pleach
RABBITMQ_DEFAULT_PASS=pleach

# Database configuration
DB_USER=pleach
DB_PASSWORD=pleach
DB_HOST=db
DB_PORT=5432
DB_NAME=pleach

# DB configuration
POSTGRES_USER=pleach
POSTGRES_PASSWORD=pleach
POSTGRES_DB=pleach
POSTGRES_PORT=5432

# Flower configuration
# Disable until https://github.com/pleach-ai/pleach/pull/2655 gets released
#PLEACH_CACHE_TYPE=redis
PLEACH_REDIS_HOST=result_backend
PLEACH_REDIS_PORT=6379
PLEACH_REDIS_DB=0
PLEACH_REDIS_EXPIRE=3600
PLEACH_REDIS_PASSWORD=
FLOWER_UNAUTHENTICATED_API=True
BROKER_URL=amqp://pleach:pleach@broker:5672
RESULT_BACKEND=redis://result_backend:6379/0
C_FORCE_ROOT="true"


# PGAdmin configuration
PGADMIN_DEFAULT_EMAIL=admin@admin.com
PGADMIN_DEFAULT_PASSWORD=admin
