{
  "keywords": [
    "def", "class", "import", "from", "if", "else", "elif", "for", "while", 
    "try", "except", "finally", "with", "as", "lambda", "return", "yield",
    "and", "or", "not", "is", "in", "global", "pass", "None", "True", "False",
    "break", "continue", "del", "assert", "async", "await", "raise", "nonlocal"
  ],
  "builtins": [
    "abs", "all", "any", "bin", "bool", "bytes", "chr", "complex", "dict", "dir",
    "divmod", "enumerate", "eval", "exec", "filter", "float", "format", "frozenset",
    "getattr", "globals", "hasattr", "hash", "help", "hex", "id", "input", "int",
    "isinstance", "issubclass", "iter", "len", "list", "locals", "map", "max", "min",
    "next", "object", "oct", "open", "ord", "pow", "print", "property", "range",
    "repr", "reversed", "round", "set", "setattr", "slice", "sorted", "staticmethod",
    "str", "sum", "super", "tuple", "type", "vars", "zip"
  ],
  "libraries": {
    "standard": [
      "os", "sys", "math", "datetime", "time", "random", "re", "json", "csv",
      "collections", "itertools", "functools", "pathlib", "shutil", "subprocess",
      "argparse", "logging", "unittest", "threading", "multiprocessing", "socket",
      "hashlib", "urllib", "http", "email", "sqlite3", "xml", "html"
    ],
    "popular": [
      "numpy", "pandas", "matplotlib", "tensorflow", "pytorch", "sklearn", "django",
      "flask", "requests", "beautifulsoup", "scipy", "sqlalchemy", "pytest", "selenium",
      "pillow", "pygame", "pyside", "pyqt", "fastapi", "celery", "pyyaml", "jinja2",
      "aiohttp", "asyncio", "boto3", "tornado", "psycopg2", "pydantic", "starlette"
    ]
  },
  "patterns": {
    "function_definition": [
      "^\\s*def\\s+([a-zA-Z_][a-zA-Z0-9_]*)\\s*\\(",
      "^\\s*async\\s+def\\s+([a-zA-Z_][a-zA-Z0-9_]*)\\s*\\("
    ],
    "class_definition": [
      "^\\s*class\\s+([a-zA-Z_][a-zA-Z0-9_]*)\\s*[:\\(]"
    ],
    "import_statement": [
      "^\\s*import\\s+([a-zA-Z_.][a-zA-Z0-9_.,\\s]*)",
      "^\\s*from\\s+([a-zA-Z_.][a-zA-Z0-9_.]*)\\s+import"
    ],
    "assignment": [
      "^\\s*([a-zA-Z_][a-zA-Z0-9_]*)\\s*=",
      "^\\s*([a-zA-Z_][a-zA-Z0-9_]*)\\s*\\+="
    ],
    "decorator": [
      "^\\s*@([a-zA-Z_][a-zA-Z0-9_]*)"
    ]
  },
  "comments": {
    "single_line": ["^\\s*#"],
    "multi_line": ["^\\s*\"\"\".*?\"\"\"", "^\\s*'''.*?'''"]
  },
  "string_literals": [
    "\".*?\"", "'.*?'", "\"\"\".*?\"\"\"", "'''.*?'''"
  ],
  "file_extensions": [".py", ".pyw", ".pyc", ".pyo", ".pyi"]
} 