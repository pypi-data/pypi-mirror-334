    "border-left", "box-shadow", "margin", "margin-top", "margin-right", "margin-bottom",
    "margin-left", "padding", "padding-top", "padding-right", "padding-bottom",
    "padding-left", "width", "height", "min-width", "min-height", "max-width", "max-height",
    "display", "position", "top", "right", "bottom", "left", "float", "clear", "z-index",
