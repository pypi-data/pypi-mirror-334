            },
            {
              "i": 1,
              "bbox": [
                300.05554623074,
                201.02221171061197,
                428.72221204969617,
                237.9222234090169
              ],
              "content": "Analysis "
            }
          ]
        },
        {
          "i": 1,
          "bbox": [
