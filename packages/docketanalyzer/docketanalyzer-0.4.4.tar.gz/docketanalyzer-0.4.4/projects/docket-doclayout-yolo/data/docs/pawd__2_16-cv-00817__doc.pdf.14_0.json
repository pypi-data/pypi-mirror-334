            {
              "i": 0,
              "bbox": [
                300.05554623074,
                1445.8277384440103,
                1508.394538031684,
                1482.7277289496528
              ],
              "content": "IT IS HEREBY ORDERED that Defendant\u2019s Motion [10] is GRANTED, IN PART, and "
            },
            {
              "i": 1,
              "bbox": [
                200.06667243109808,
                1524.494425455729,
                1508.5445827907986,
                1561.3944159613716
              ],
              "content": "DENIED, IN PART.  Defendant\u2019s Motion to Dismiss Plaintiff\u2019s \u00a7 1983 excessive force claim at "
            },
            {
              "i": 2,
              "bbox": [
                200.06667243109808,
                1603.1611124674478,
                1508.0724080403645,
                1640.0611029730903
              ],
              "content": "Count I is denied as the Court finds that Plaintiff has set forth sufficient allegations to state a "
            },
            {
              "i": 3,
              "bbox": [
                200.06667243109808,
                1681.9110446506077,
                1508.627658420139,
                1718.81103515625
              ],
              "content": "plausible claim that Officer Kreger\u2019s actions in directing Plaintiff to remain in his car and thereafter "
            },
            {
              "i": 4,
              "bbox": [
                200.06667243109808,
                1760.5777316623264,
                1508.6110432942708,
                1797.4777221679688
              ],
              "content": "intentionally slamming the car door, causing injury to his arm, constituted an actionable \u201cseizure\u201d "
            },
            {
              "i": 5,
              "bbox": [
                200.06667243109808,
                1839.2444186740452,
                1508.6110432942708,
                1876.1444091796875
              ],
              "content": "under Third Circuit jurisprudence.  See United States v. Brown, 448 F.3d 239, 245 (3d Cir. 2006) "
            }
          ]
