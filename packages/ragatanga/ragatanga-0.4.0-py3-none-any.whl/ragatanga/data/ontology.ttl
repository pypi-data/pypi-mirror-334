@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://www.semanticweb.org/ontologies/pratique-fitness/> .

###############################################################################
## Ontology Declaration - Fix invalid URIs
###############################################################################
<http://www.semanticweb.org/ontologies/pratique-fitness/> rdf:type owl:Ontology ;
    rdfs:label "Pratique Fitness Full Ontology" ;
    owl:versionInfo "2.6" ;
    owl:versionIRI <http://www.semanticweb.org/ontologies/pratique-fitness/2.6> .

###############################################################################
## Classes
###############################################################################
# Note: After reviewing the class hierarchy, no circular inheritance issues were found.
# Base classes should be defined first
:Modalidade rdf:type owl:Class ;
    rdfs:label "Modalidade" ;
    rdfs:comment "Atividades e modalidades oferecidas (ex.: Musculação, Dança, Natação, etc.)." .

# Then subclasses
:PoolActivity rdf:type owl:Class ;
    rdfs:subClassOf :Modalidade ;  # This is the correct way to define inheritance
    rdfs:label "Atividade Aquática" ;
    rdfs:comment "Modalidades relacionadas ao uso da piscina." .

:Unidade rdf:type owl:Class ;
    rdfs:label "Unidade" ;
    rdfs:comment "Uma unidade física da Pratique Fitness." .

:UnidadeTipo rdf:type owl:Class ;
    rdfs:label "Tipo de Unidade" ;
    rdfs:comment "Classifica as unidades em: Slim, Premium e Prime/ Espaço Prime." .

:Plano rdf:type owl:Class ;
    rdfs:label "Plano / Assinatura" ;
    rdfs:comment "Planos e assinaturas disponíveis para clientes." .

:Benefit rdf:type owl:Class ;
    rdfs:label "Benefício" ;
    rdfs:comment "Benefícios e serviços que integram os planos." .

:PaymentMethod rdf:type owl:Class ;
    rdfs:label "Forma de Pagamento" ;
    rdfs:comment "Métodos de pagamento (cartão de crédito, PIX, etc.)." .

:PratiqueNutriService rdf:type owl:Class ;
    rdfs:label "Serviço Pratique Nutri" ;
    rdfs:comment "Serviço de acompanhamento nutricional, via app Dietbox." .

:ModalidadeType rdf:type owl:Class ;
    rdfs:label "Tipo de Modalidade" ;
    rdfs:comment "Classifica as modalidades (ex.: Regular, Terceirizada)." .

:Pool rdf:type owl:Class ;
    rdfs:label "Piscina" ;
    rdfs:comment "Informações sobre as piscinas disponíveis nas unidades." .

:BikeRental rdf:type owl:Class ;
    rdfs:label "Locação de Bike (PowerCycle)" ;
    rdfs:comment "Serviço de locação de bikes, com planos e benefícios." .

:PersonalExternoService rdf:type owl:Class ;
    rdfs:label "Serviço de Personal Externo" ;
    rdfs:comment "Programa para personal trainers atuarem nas unidades." .

:PersonalClassService rdf:type owl:Class ;
    rdfs:label "Serviço de Personal Class" ;
    rdfs:comment "Programa de aulas personalizadas via créditos." .

:JumperFitService rdf:type owl:Class ;
    rdfs:label "Serviço Jumper Fit" ;
    rdfs:comment "Recreação esportiva com trampolins (para todas as idades)." .

:ExameBioimpedancia rdf:type owl:Class ;
    rdfs:label "Exame de Bioimpedância" ;
    rdfs:comment "Exame para avaliar composição corporal (avulso ou incluso em planos)." .

:History rdf:type owl:Class ;
    rdfs:label "História" ;
    rdfs:comment "Histórico e informações sobre a evolução da Pratique Fitness." .

###############################################################################
## Additional Classes for Support, Age Policy, etc.
###############################################################################
:GeneralSupport rdf:type owl:Class ;
    rdfs:subClassOf owl:Thing ;
    rdfs:label "Suporte Geral" ;
    rdfs:comment "Canal para informações e dúvidas diversas." .

:AgePolicy rdf:type owl:Class ;
    rdfs:label "Política de Idade" ;
    rdfs:comment "Regras para menores de idade e condições de ingresso." .

:TreatmentType rdf:type owl:Class ;
    rdfs:label "Tipo de Tratamento" .

:PoolSchedule rdf:type owl:Class ;
    rdfs:label "Horário da Piscina" .

###############################################################################
## Object Properties
###############################################################################
:hasType rdf:type owl:FunctionalObjectProperty ;
    rdfs:domain :Unidade ;
    rdfs:range :UnidadeTipo ;
    rdfs:label "tem tipo" ;
    rdfs:comment "Relaciona uma unidade ao seu tipo (Slim, Premium, Prime)." .

:hasPlan rdf:type owl:ObjectProperty ;
    rdfs:domain :Unidade ;
    rdfs:range :Plano ;
    rdfs:label "tem plano" ;
    rdfs:comment "Indica que a unidade oferece determinado plano." ;
    owl:inverseOf :availableIn .

:availableIn rdf:type owl:ObjectProperty ;
    rdfs:domain :Plano ;
    rdfs:range :Unidade ;
    rdfs:label "disponível em" ;
    rdfs:comment "Indica as unidades onde um plano está disponível." ;
    owl:inverseOf :hasPlan .

:includesBenefit rdf:type owl:AsymmetricObjectProperty,
                          owl:IrreflexiveObjectProperty ;
    rdfs:domain :Plano ;
    rdfs:range :Benefit ;
    rdfs:label "inclui benefício" ;
    rdfs:comment "Relaciona um plano aos seus benefícios." .

:isBenefitOf rdf:type owl:ObjectProperty ;
    rdfs:domain :Benefit ;
    rdfs:range :Plano ;
    rdfs:label "é benefício de" .

:requiresPaymentMethod rdf:type owl:ObjectProperty ;
    rdfs:domain :Plano ;
    rdfs:range :PaymentMethod ;
    rdfs:label "requer forma de pagamento" .

:isIncludedIn rdf:type owl:ObjectProperty ;
    rdfs:domain :PratiqueNutriService ;
    rdfs:range :Plano ;
    rdfs:label "está incluído em" .

:offeredAt rdf:type owl:ObjectProperty ;
    rdfs:domain :Modalidade ;
    rdfs:range :Unidade ;
    rdfs:label "oferecido em" ;
    rdfs:comment "Indica em quais unidades a modalidade é oferecida." .

:hasModalityType rdf:type owl:ObjectProperty ;
    rdfs:domain :Modalidade ;
    rdfs:range :ModalidadeType ;
    rdfs:label "tem tipo de modalidade" ;
    rdfs:comment "Relaciona uma modalidade ao seu tipo (ex.: Regular, Terceirizada)." .

:hasPool rdf:type owl:AsymmetricObjectProperty,
                 owl:IrreflexiveObjectProperty ;
    rdfs:domain :Unidade ;
    rdfs:range :Pool ;
    rdfs:label "possui piscina" ;
    rdfs:comment "Indica que a unidade possui uma ou mais piscinas." .

:isPoolOf rdf:type owl:ObjectProperty ;
    rdfs:domain :Pool ;
    rdfs:range :Unidade ;
    rdfs:label "piscina da unidade" .

:hasTreatment rdf:type owl:ObjectProperty ;
    rdfs:domain :Pool ;
    rdfs:range :TreatmentType ;
    rdfs:label "tipo de tratamento" .

:supportsActivity rdf:type owl:ObjectProperty ;
    rdfs:domain :Pool ;
    rdfs:range :PoolActivity ;
    rdfs:label "suporta atividade" ;
    rdfs:comment "Indica quais atividades podem ser praticadas na piscina." .

:hasRequirement rdf:type owl:ObjectProperty ;
    rdfs:domain owl:Thing ;
    rdfs:range :PoolRequirement ;
    rdfs:label "tem requisito" ;
    rdfs:comment "Relaciona uma piscina (ou unidade) aos requisitos para seu uso." .

:hasCancellationForm rdf:type owl:ObjectProperty ;
    rdfs:domain owl:Thing ;
    rdfs:range rdfs:Resource ;
    rdfs:label "formulário de cancelamento" .

:operatesIn rdf:type owl:ObjectProperty ;
    rdfs:domain :PersonalExternoService ;
    rdfs:range :Unidade ;
    rdfs:label "opera em" ;
    rdfs:comment "Unidades em que o Personal Externo pode atuar." .

:notOfferedAt rdf:type owl:ObjectProperty ;
    rdfs:domain :Modalidade ;
    rdfs:range :Unidade ;
    rdfs:label "não oferecido em" ;
    rdfs:comment "Indica em quais unidades a modalidade NÃO é oferecida (exceções)." .

###############################################################################
## Datatype Properties
###############################################################################
:hasLink rdf:type owl:DatatypeProperty ;
    rdfs:domain :PratiqueNutriService ;
    rdfs:range xsd:anyURI ;
    rdfs:label "tem link" ;
    rdfs:comment "Link para agendamento, consulta ou informações adicionais." .

:hasAddress rdf:type owl:DatatypeProperty ;
    rdfs:domain :Unidade ;
    rdfs:range xsd:string ;
    rdfs:label "possui endereço" .

:hasCity rdf:type owl:DatatypeProperty ;
    rdfs:domain :Unidade ;
    rdfs:range xsd:string ;
    rdfs:label "está na cidade" .

:hasState rdf:type owl:DatatypeProperty ;
    rdfs:domain :Unidade ;
    rdfs:range xsd:string ;
    rdfs:label "está no estado" .

:hasWorkingHours rdf:type owl:DatatypeProperty ;
    rdfs:domain :Unidade ;
    rdfs:range xsd:string ;
    rdfs:label "horário de funcionamento" .

:hasPrice rdf:type owl:DatatypeProperty ;
    rdfs:domain :Plano ;
    rdfs:range xsd:decimal ;
    rdfs:label "tem preço" ;
    rdfs:comment "Valor do plano em reais." .

:hasRecurrence rdf:type owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain :Plano ;
    rdfs:range xsd:string ;
    rdfs:label "tem recorrência" ;
    rdfs:comment "Frequência do pagamento (ex.: Mensal, Semanal)." .

:hasPricingDetail rdf:type owl:DatatypeProperty ;
    rdfs:domain :Plano ;
    rdfs:range xsd:string ;
    rdfs:label "detalhes de precificação" ;
    rdfs:comment "Informações detalhadas sobre a precificação, inclusive exceções por unidade." .

:hasPaymentComment rdf:type owl:DatatypeProperty ;
    rdfs:domain :PaymentMethod ;
    rdfs:range xsd:string ;
    rdfs:label "comentário sobre pagamento" .

:hasUnitException rdf:type owl:DatatypeProperty ;
    rdfs:domain :Plano ;
    rdfs:range xsd:string ;
    rdfs:label "exceção de unidade" ;
    rdfs:comment "Exceções de preço para determinadas unidades." .

:hasContactNumber rdf:type owl:DatatypeProperty ;
    rdfs:domain :GeneralSupport ;
    rdfs:range xsd:string ;
    rdfs:label "número de contato" .

:hasContactChannel rdf:type owl:DatatypeProperty ;
    rdfs:domain :GeneralSupport ;
    rdfs:range xsd:string ;
    rdfs:label "canal de contato" .

:handlesInquiries rdf:type owl:DatatypeProperty ;
    rdfs:domain :GeneralSupport ;
    rdfs:range xsd:string ;
    rdfs:label "assuntos atendidos" .

:hasNoticePeriod rdf:type owl:DatatypeProperty ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string ;
    rdfs:label "período de aviso prévio" .

:hasLockBonus rdf:type owl:DatatypeProperty ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:decimal ;
    rdfs:label "bônus de trancamento" .

:openingTime rdf:type owl:DatatypeProperty ;
    rdfs:domain :PoolSchedule ;
    rdfs:range xsd:string ;
    rdfs:label "horário de abertura" .

:closingTime rdf:type owl:DatatypeProperty ;
    rdfs:domain :PoolSchedule ;
    rdfs:range xsd:string ;
    rdfs:label "horário de fechamento" .

:hasAlternatePrice rdf:type owl:DatatypeProperty ;
    rdfs:domain :Plano ;
    rdfs:range xsd:decimal ;
    rdfs:label "tem preço alternativo" ;
    rdfs:comment "Preço alternativo (ex.: diárias em unidades Prime) para um plano." .

###############################################################################
## New Datatype Property for Unit Pricing Detail
