	<xsd:complexType name="EntryParamsType">
		<xsd:annotation>
			<xsd:documentation>
				Желаемые параметры зачисления
            </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="EntryDate" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Желаемая дата зачисления</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Language" type="tns:DataElementType">
				<xsd:annotation>
					<xsd:documentation>Язык обучения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Schedule" type="tns:DataElementType">
				<xsd:annotation>
					<xsd:documentation>Режим пребывания в группе</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AgreementOnFullDayGroup" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Согласие на зачисление в группы полного дня (Старый ВС)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AgreementOnOtherDayGroup" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Согласие на зачисление в группы с другим режимом (новый ВС)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AdaptationProgramType">
		<xsd:annotation>
			<xsd:documentation>Направленность группы
            </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AdaptationGroup" type="tns:DataElementType">
				<xsd:annotation>
					<xsd:documentation>Группа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AdaptationGroupType" type="tns:DataElementType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Вид/профиль группы</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AgreementAdaptationEducationGroup" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Согласие на обучение по адаптированной образовательной программе</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AgreementOnGeneralGroup" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Согласие на зачисление в общеразвивающую или комбинированную группу</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AgreementOnCareGroup" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Группа без реализации образовательной программы</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NeedSpecialCareConditions" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Требуется создание специальных условий для организации обучения и воспитания ребенка-инвалида в соответствии с индивидуальной программой реабилитации инвалида</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
