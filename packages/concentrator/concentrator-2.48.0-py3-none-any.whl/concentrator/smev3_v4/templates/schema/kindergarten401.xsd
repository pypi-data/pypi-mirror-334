					<xsd:documentation>Кем выдан документ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocExpirationDate" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата окончания документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
