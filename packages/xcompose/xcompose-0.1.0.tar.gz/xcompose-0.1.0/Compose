<Multi_key> <m> <a> <q> : "--¬∑-"   # MORSE CODE Q
<Multi_key> <m> <a> <r> : "¬∑-¬∑"    # MORSE CODE R
<Multi_key> <m> <a> <s> : "¬∑¬∑¬∑"    # MORSE CODE S
