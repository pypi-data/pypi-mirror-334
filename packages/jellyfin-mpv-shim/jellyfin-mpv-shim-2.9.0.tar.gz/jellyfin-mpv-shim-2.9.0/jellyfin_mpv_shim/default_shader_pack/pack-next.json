{
    "setting-groups": {
        "deband-default": {
            "settings": [
                ["deband", true],
                ["deband_grain", 0],
                ["deband_range", 12],
                ["deband_threshold", 32]
            ]
        },
        "hwdec-default": {
            "settings": [
                ["hwdec", "auto-copy"],
                ["profile", "gpu-hq"],
                ["gpu_api", "opengl"],
                ["fbo_format", "rgba16f"]
            ]
        },
        "static-grain-default": {
            "shaders": [
                "noise_static_luma.hook",
                "noise_static_chroma.hook"
            ]
        },
        "static-grain-strong": {
            "shaders": [
                "noise_static_luma_strong.hook",
                "noise_static_chroma_strong.hook"
            ]
        },
        "fsrcnnx-high": {
            "shaders": [
                "FSRCNNX_x2_16-0-4-1.glsl"
            ],
            "settings": [
                ["scale", "ewa_lanczos"]
            ]
        },
        "fsrcnnx": {
            "shaders": [
                "FSRCNNX_x2_8-0-4-1.glsl"
            ],
            "settings": [
                ["scale", "ewa_lanczos"]
            ]
        },
        "ssim-downscaler": {
            "shaders": [
                "SSimDownscaler.glsl"
            ],
            "settings": [
                ["dscale", "mitchell"],
                ["linear_downscaling", "no"]
            ]
        },
        "krig-bilateral": {
            "shaders": [
                "KrigBilateral.glsl"
            ],
            "settings": [
                ["cscale", "mitchell"]
            ]
        },
        "dither-fruit-default": {
            "settings": [
                ["dither_depth", "auto"],
                ["dither", "fruit"]
            ]
        },
        "tscale-box": {
            "settings": [
                ["interpolation", true],
                ["video_sync", "display-resample"],
                ["tscale", "box"],
                ["tscale_window", "quadric"],
                ["tscale_clamp", 0.0],
                ["tscale_radius", 1.025]
            ]
        },
        "nnedi-low": {
            "shaders": [
                "nnedi3-nns16-win8x6.hook"
            ]
        },
        "nnedi-medium": {
            "shaders": [
                "nnedi3-nns32-win8x6.hook"
            ]
        },
        "nnedi-high": {
            "shaders": [
                "nnedi3-nns64-win8x6.hook"
            ]
        },
        "nnedi-very-high": {
            "shaders": [
                "nnedi3-nns128-win8x6.hook"
            ]
        },
        "nnedi-placebo": {
            "shaders": [
                "nnedi3-nns256-win8x6.hook"
            ]
        }
    },
    "default-setting-groups": [
        "deband-default",
        "hwdec-default",
        "static-grain-default",
        "dither-fruit-default"
    ],
    "setting-revert-ignore": [
        "profile",
        "dscale",
        "gpu_api"
